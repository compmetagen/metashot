#!/usr/bin/env expect
set timeout 10
spawn checkm data setRoot
expect "It seems that the CheckM data folder has not been set yet or has been removed. Running: 'checkm data setRoot'.\nWhere should CheckM store it's data?\nPlease specify a location or type 'abort' to stop trying: \n" {send "/usr/local/share/checkm_data_2015_01_16\r"}
interact


